@model FNC_AB_ORDER_MANAGEMENT.Models.AnvandareModel
@{
    ViewBag.Title = "Visa Användare";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Visa Användare</h2>

<table id="example" class="display" cellspacing="0" width="100%">
    <thead>
    <tr>
        <th>Username</th>
        <th>Email</th>
        
      
    </tr>
    </thead>
    <tfoot>
    <tr>
        <th>Username</th>
        <th>Email</th>
       
     
    </tr>
    </tfoot>
    <tbody>
    @{ 
        if (Model != null)
        {
            foreach (var item in Model.AnvandareLista)
            {
                    
              
                <tr>
                    <td Onclick="location.href='@Url.Action("RedigeraAnvandare/"+item.Id, "HanteraKonton")'">@item.UserName</td>
                  
                    <td>@item.Email</td>
                    
                 
                </tr>

            }
        }
    }

    </tbody>
</table>

<script>
    $(document).ready(function() {
        $('#example').DataTable();
    } );
</script>   





